<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyecto">
    <uses-permission android:name="android.permission.INTERNET"/> <!--se agrega para que se pueda conectar a base de datos-->

    <application
        android:allowBackup="true"
        android:icon="@drawable/fonet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Juegos"></activity>
        <activity android:name=".MenuPrincipal" />
        <activity android:name=".Registro" />
        <activity android:name=".RecuperarContrasena" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>